<div class="container">
  <div id="user_nav">
  
    <% if user_signed_in? %>
      <a class="btn btn-xs btn-default"> Signed in as <%= current_user.email %>. Not you? </a>
      <%= link_to "Sign Out", destroy_user_session_path, :method => :delete, :class => "btn btn-xs btn-default" %>
    <% else %>
      <%= link_to "Sign Up", new_user_registration_path, :class => "btn btn-default" %>
      <%= link_to "Sign In", new_user_session_path, :class => "btn btn-default" %>
      <!-- < % = render 'partials/yield' % > -->
    <% end %>

  </div>
    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}", :class => "btn btn-xs btn-default" %>
    <% end %>
</div>